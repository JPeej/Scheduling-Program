<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="450.0" prefWidth="290.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ModifyCustomerController">
    <children>
        <Label layoutX="40.0" layoutY="74.0" text="ID" />
      <Label layoutX="40.0" layoutY="114.0" text="Name" />
        <Label layoutX="40.0" layoutY="194.0" text="Address" />
        <Label layoutX="40.0" layoutY="234.0" text="Zipcode" />
        <Label layoutX="40.0" layoutY="154.0" text="Phone" />
        <Label layoutX="53.0" layoutY="20.0" text="Modify Customer" underline="true">
            <font>
                <Font size="24.0" />
            </font></Label>
        <TextField fx:id="idText" disable="true" editable="false" layoutX="100.0" layoutY="70.0" />
      <TextField fx:id="nameText" layoutX="100.0" layoutY="110.0" />
        <TextField fx:id="phoneText" layoutX="100.0" layoutY="150.0" />
        <TextField fx:id="addressText" layoutX="99.0" layoutY="190.0" />
        <TextField fx:id="zipText" layoutX="99.0" layoutY="230.0" />
        <ComboBox fx:id="countryCombo" layoutX="40.0" layoutY="269.0" onAction="#onActionCountryCombo" prefHeight="26.0" prefWidth="210.0" promptText="Country" />
        <ComboBox fx:id="divCombo" layoutX="40.0" layoutY="310.0" prefHeight="26.0" prefWidth="210.0" promptText="State / Province" />
        <Button layoutX="45.0" layoutY="386.0" mnemonicParsing="false" onAction="#onActionSaveCustomer" prefWidth="75.0" text="Save" />
        <Button layoutX="172.0" layoutY="386.0" mnemonicParsing="false" onAction="#onActionCancel" prefWidth="75.0" text="Cancel" />
    </children>
</AnchorPane>
